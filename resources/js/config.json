{
    "app_name": "Notebook", 
    "app_url": "http://127.0.0.1:8000", 
    "locale": {
        "default": "ru", 
        "fallback": "en"
    }, 
    "routes": {
        "api": {
            "prefix": "api"
        }
    }, 
    "cache": {
        "prefix": "__cache_", 
        "separator": "_"
    }, 
    "database": {
        "cache": {
            "prefix": "database_"
        }
    }, 
    "model": {
        "primary_key": "id", 
        "created_at": "created_at", 
        "updated_at": "updated_at", 
        "deleted_at": "deleted_at", 
        "cache": {
            "prefix": "model_"
        }, 
        "uuid": {
            "prefix": "id:"
        }, 
        "trashed": {
            "prefix": "trash"
        }
    }, 
    "notebook": {
        "cache": {
            "prefix": "__cache_notebook_"
        }, 
        "autosave": {
            "default": false, 
            "interval": 60000
        }
    }, 
    "window": {
        "open": true, 
        "fullscreen": false, 
        "statusbar": true, 
        "toolbar": false, 
        "standard": false, 
        "tabs": true, 
        "resize": {
            "interval": 200
        }, 
        "cache": {
            "prefix": "__cache_window_"
        }
    }, 
    "sidebar": {
        "show": {
            "notes": true, 
            "search": false, 
            "trash": false
        }, 
        "detailed": true, 
        "cache": {
            "prefix": "__cache_sidebar_"
        }
    }, 
    "sort": {
        "columns": [
            "title", 
            "updated_at"
        ], 
        "directions": [
            "desc", 
            "asc"
        ]
    }, 
    "notification": {
        "timeout": true, 
        "interval": 2000
    }, 
    "mark": {
        "options": {
            "element": "span", 
            "className": "bg-indigo-200 dark:bg-indigo-900", 
            "acrossElements": true, 
            "caseSensitive": false, 
            "separateWordSearch": false
        }
    }, 
    "editable": {
        "commands": [
            "bold",
            "italic",
            "underline",
            "strikethrough",
            "superscript",
            "subscript",
            "insertUnorderedList",
            "insertOrderedList",
            "formatBlock",
            "insertHorizontalRule",
            "insertImage",
            "createLink",
            "unlink",
            "insertText",
            "insertHTML",
            "justifyLeft",
            "justifyCenter",
            "justifyRight",
            "justifyFull",
            "fontName",
            "fontSize",
            "foreColor",
            "hiliteColor",
            "undo",
            "redo",
            "selectAll",
            "delete",
            "removeFormat",
            "cut",
            "copy"
        ], 
        "options": {
            "showUI": false,
            "checkSupport": true,
            "notify": true,
            "cssMode": false,
            "paragraphSeparator": "p",
            "spellcheck": true,
            "autofocus": false,
            "successNotification": null,
            "errorNotification": null
        }
    }
}